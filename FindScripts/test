local Library = loadstring(game:HttpGet(
    'https://raw.githubusercontent.com/VisualRoblox/Roblox/main/UI-Libraries/Visual%20Command%20UI%20Library/Source.lua',
    true))()

local Window = Library:CreateWindow({
    Name = 'FindScripts',
    IntroText = 'Find scripts by just_nev_dev',
    IntroIcon = 'rbxassetid://11963373994',
    IntroBlur = true,
    IntroBlurIntensity = 15,
    Theme = Library.Themes.dark,
    Position = 'bottom',
    Draggable = true,
    Prefix = ','
})


local lockedcmds = true
local correctKeyEntered = false

local function verifyKey()
    while lockedcmds do
        wait(10)

        if correctKeyEntered then
            break
        end
    end
end


-- // Commands
Window:AddCommand('SetPrefix', { 'New Prefix' }, 'Cambia la prefix.', function(Arguments, Speaker)
    Window:ChangePrefix(Arguments[1])
end)

Window:AddCommand('SetTheme', { 'New Theme' }, 'Cambia el tema', function(Arguments, Speaker)
    Window:ChangeTheme(Arguments[1])
end)

Window:AddCommand('Key ', { 'Keyinserted' }, 'Inserta la clave para acceder a los demas comandos.',
    function(Arguments, Speaker)
        if Arguments[1] == 'KS' then
            correctKeyEntered = true
            lockedcmds = false
            print("Key inserted is correct.")
        else
            print("Key inserted is incorrect.")
        end
    end)

spawn(verifyKey)

if not lockedcmds then
    Window:AddCommand('test', {}, 'test', function(Arguments, Speaker)
        Window:CreateNotification('test', 'Notification', 5)
    end)
else
    print("Commands are locked")
end

-- // Functions
Window:AddTheme('normal', {
    BackgroundColor = Color3.fromRGB(0, 255, 255),
    SecondaryColor = Color3.fromRGB(225, 225, 225),
    AccentColor = Color3.fromRGB(125, 125, 125),
    PrimaryTextColor = Color3.fromRGB(0, 0, 0),
    SecondaryTextColor = Color3.fromRGB(75, 75, 75)
})

Window:AddTheme('blue', {
    BackgroundColor = Color3.fromRGB(0, 255, 255),
    SecondaryColor = Color3.fromRGB(173, 209, 209),
    AccentColor = Color3.fromRGB(96, 131, 131),
    PrimaryTextColor = Color3.fromRGB(0, 0, 0),
    SecondaryTextColor = Color3.fromRGB(75, 75, 75)
})

Window:AddTheme('black', {
    BackgroundColor = Color3.fromRGB(231, 231, 231),
    SecondaryColor = Color3.fromRGB(190, 190, 190),
    AccentColor = Color3.fromRGB(125, 125, 125),
    PrimaryTextColor = Color3.fromRGB(0, 0, 0),
    SecondaryTextColor = Color3.fromRGB(75, 75, 75)
})

for _, Theme in next, Window:GetThemes(true) do
    print(Theme)
end

for Index, Theme in next, Window:GetThemes(false) do
    print(Index, Theme)
end
